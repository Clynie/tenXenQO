<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.60"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tenXen.client.controller.LoginControl">
    <children>
        <TitledPane animated="false" collapsible="false" text="登入">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <GridPane AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
                            <columnConstraints>
                                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="73.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="127.0" minWidth="10.0" prefWidth="127.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Label text="用户名："/>
                                <Label text="密码：" GridPane.rowIndex="1"/>
                                <TextField fx:id="userName" GridPane.columnIndex="1"/>
                                <TextField fx:id="pwd" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            </children>
                        </GridPane>
                        <Button fx:id="login" layoutX="175.0" layoutY="85.0" mnemonicParsing="false" onAction="#doLogin" text="登入"/>
                        <Button fx:id="register" layoutX="115.0" layoutY="85.0" mnemonicParsing="false" onAction="#register" text="注册"/>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <TextArea fx:id="output" editable="false" prefHeight="100.0" prefWidth="300.0"/>
    </children>
</VBox>
